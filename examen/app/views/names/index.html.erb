<p id="notice"><%= notice %></p>

<div class="master-container">
<h1 class="titles">Names</h1>

<table>
  <thead>
    <tr>
      <th>Planet Name   </th>
      <th>Moon Names    </th>
      <th>Star Names    </th>
      <th>System ID   </th>
      <th>System Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name.planetNames %></td>
        <td><%= name.moonNames %></td>
        <td><%= name.starNames %></td>
        <td><%= name.system_id%></td>
        <td><%= name.system.name %></td>
        <td class="url-links"><%= link_to 'Show', name %></td>
        <td class="url-links"><%= link_to 'Edit', edit_name_path(name) %></td>
        <td class="url-links"><%= link_to 'Destroy', name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<div class="new-system">
  <%= link_to 'New Name', new_name_path %>  
</div>
